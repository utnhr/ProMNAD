Geometry = GenFormat {
    <<< "geo.gen"
}

Hamiltonian = DFTB {
    SCC = Yes
    SCCTolerance = 1e-10
    MaxAngularMomentum = {
        C  = "p"
        H  = "s"
        O  = "p"
    }
    SlaterKosterFiles = Type2FileNames {
        Prefix = "./"
        Separator = "-"
        Suffix = ".skf"
    }
}

Options {
    WriteHS = Yes
}

Parallel {
  UseOmpThreads = Yes
}

